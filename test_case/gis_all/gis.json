{
    "gis_all" :
    {
        "single_polygon":
        {
            "file_path": "data/single_polygon.csv",
            "funcs_sql":
            [
                [
                    "st_area",
                    "select ST_Area(ST_GeomFromText(geos)) from %s"
                ],
                [
                    "st_astext",
                    "select ST_AsText(ST_GeomFromText(geos)) from %s"
                ],
                [
                    "st_envelope",
                    "select ST_AsText(ST_Envelope(ST_GeomFromText(geos))) from %s"
                ],
                [
                    "st_geomfromwkt",
                    "select ST_AsText(ST_GeomFromWKT(geos)) from %s"
                ],
                [
                    "st_issimple",
